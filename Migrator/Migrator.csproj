<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/arian2ashk/SystemDataSqlite.To.SqlCipher</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>System.Data.SQLite, Sqlite, SqlCipher, Microsoft.Data.Sqlite</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="5.0.3" />
    <PackageReference Include="SQLitePCLRaw.bundle_e_sqlcipher" Version="2.0.4" />
    <PackageReference Include="System.Data.SQLite" Version="1.0.112" />
  </ItemGroup>

</Project>
